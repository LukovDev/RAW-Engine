// Объявление функций:
// ...


// Создать TMPLT:
TemplateS* TemplateS_create() {
    TemplateS *tmp = (TemplateS*)mm_alloc(sizeof(TemplateS));

    // ...

    return tmp;
}


// Уничтожить TMPLT:
void TemplateS_destroy(TemplateS **tmp) {
    if (!tmp || !*tmp) return;
    mm_free(*tmp);
    *tmp = NULL;
}


